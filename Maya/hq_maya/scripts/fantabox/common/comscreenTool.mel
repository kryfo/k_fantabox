global proc nishizhen5(){
   global int $iD;
   global int $iL;
   global int $iU;
   global int $iR;
   global int $iC;
   global int $iD_R;
   global int $iL_R;
   global int $iU_R;
   global int $iR_R;
   global int $iC_R;
$iD=$iD+1;
$iL=$iL+1;
$iU=$iU+1;
$iR=$iR+1;
if($iD>=5){$iD=1;}
if($iL>=5){$iL=1;}
